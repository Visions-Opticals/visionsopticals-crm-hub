{
  "name": "dorcas/hub-base",
  "description": "The Dorcas Hub Base Edition",
  "keywords": ["framework", "laravel", "dorcas"],
  "license": "MIT",
  "type": "project",
  "repositories": [
  ],
  "require": {
    "php": "^7.2|^8.1.7",
    "ext-json": "*",
    "aws/aws-sdk-php": "^3.52",
    "barryvdh/laravel-dompdf": "^0.8.7",
    "bref/bref": "^1.3",
    "bref/laravel-bridge": "^1.1",
    "bugsnag/bugsnag-laravel": "^2.0",
    "dorcas/laravel-compat": "dev-dev-ola",
    "dorcas/modules-access-requests": "dev-dev-ola",
    "dorcas/modules-app-store": "dev-dev-ola",
    "dorcas/modules-assistant": "dev-dev-ola",
    "dorcas/modules-auth": "dev-dev-ola",
    "dorcas/modules-customers": "dev-dev-ola",
    "dorcas/modules-dashboard": "dev-dev-ola",
    "dorcas/modules-ecommerce": "dev-dev-ola",
    "dorcas/modules-finance": "dev-dev-ola",
    "dorcas/modules-finance-tax": "dev-dev-ola",
    "dorcas/modules-integrations": "dev-dev-ola",
    "dorcas/modules-library": "dev-dev-ola",
    "dorcas/modules-marketplace": "dev-dev-ola",
    "dorcas/modules-people": "dev-dev-ola",
    "dorcas/modules-people-payroll": "dev-dev-ola",
    "dorcas/modules-sales": "dev-dev-ola",
    "dorcas/modules-service-profile": "dev-dev-ola",
    "dorcas/modules-service-requests": "dev-dev-ola",
    "dorcas/modules-settings": "dev-dev-ola",
    "dorcas/resellerclub-php": "dev-master",
    "dorcas/sdk-php-laravel": "dev-dev-ola",
    "fedeisas/laravel-mail-css-inliner": "^4.0",
    "laravel/tinker": "^2.5",
    "fideloper/proxy": "~4.0",
    "guzzlehttp/guzzle": "^6.3",
    "kingflamez/laravelrave": "^4.2",
    "laravel/framework": "^8.0",
    "laravel/helpers": "^1.4",
    "laravel/passport": "^10.4.1",
    "laravel/ui": "^3.0",
    "lcobucci/jwt": "^4.1.4",
    "league/csv": "^9.1",
    "moneyphp/money": "^3.1",
    "moontoast/math": "1.1.2 as 1.99.0",
    "predis/predis": "^1.1",
    "ramsey/uuid": "^4.2.2",
    "yabacon/paystack-php": "^2.1"
  },
  "require-dev": {
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "^9.5.10"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/Dorcas/Support/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover",
      "@php artisan vendor:publish --tag=dorcas-modules --force"
    ]
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "cache-files-ttl":0
  }
}
