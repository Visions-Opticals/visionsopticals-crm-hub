{
    "name": "dorcas/hub-enterprise",
    "description": "The Dorcas Hub Enterprise Edition",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories":[

    ],
    "require": {
        "php": ">=7.0.0",
        "ext-json": "*",
        "aws/aws-sdk-php": "^3.52",
        "bugsnag/bugsnag-laravel": "^2.0",
        "dorcas/laravel-compat": "dev-master",
        "dorcas/modules-access-requests": "dev-develop",
        "dorcas/modules-app-store": "dev-develop",
        "dorcas/modules-assistant": "dev-develop",
        "dorcas/modules-customers": "dev-develop",
        "dorcas/modules-integrations": "dev-develop",
        "dorcas/modules-library": "dev-develop",
        "dorcas/modules-people": "dev-develop",
        "dorcas/modules-service-requests": "dev-develop",
        "dorcas/modules-service-profile": "dev-develop",
        "dorcas/resellerclub-php": "dev-master",
        "dorcas/sdk-php-laravel": "dev-master",
        "fedeisas/laravel-mail-css-inliner": "^2.2",
        "fideloper/proxy": "~3.3",
        "guzzlehttp/guzzle": "^6.3",
        "kingflamez/laravelrave": "^1.0",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "league/csv": "^9.1",
        "moneyphp/money": "^3.1",
        "moontoast/math": "^1.1",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^3.7",
        "yabacon/paystack-php": "^2.1"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Dorcas/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "minimum-stability": "dev",
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
