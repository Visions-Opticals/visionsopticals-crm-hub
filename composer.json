{
    "name": "dorcas/hub-base",
    "description": "The Dorcas Hub Base Edition",
    "keywords": ["framework", "laravel", "dorcas"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "aws/aws-sdk-php": "^3.52",
        "barryvdh/laravel-dompdf": "^0.8.7",
        "bref/bref": "^1.3",
        "bref/laravel-bridge": "^1.1",
        "bugsnag/bugsnag-laravel": "^2.0",
        "dorcas/laravel-compat": "dev-dev",
        "dorcas/modules-access-requests": "dev-dev",
        "dorcas/modules-app-store": "dev-dev",
        "dorcas/modules-assistant": "dev-dev",
        "dorcas/modules-auth": "dev-dev",
        "dorcas/modules-customers": "dev-dev",
        "dorcas/modules-dashboard": "dev-dev",
        "dorcas/modules-ecommerce": "dev-dev",
        "dorcas/modules-finance": "dev-dev",
        "dorcas/modules-finance-tax": "dev-dev",
        "dorcas/modules-integrations": "dev-dev",
        "dorcas/modules-library": "dev-dev",
        "dorcas/modules-marketplace": "dev-dev",
        "dorcas/modules-people": "dev-dev",
        "dorcas/modules-people-payroll": "dev-dev",
        "dorcas/modules-sales": "dev-dev",
        "dorcas/modules-service-profile": "dev-dev",
        "dorcas/modules-service-requests": "dev-dev",
        "dorcas/modules-settings": "dev-dev",
        "dorcas/resellerclub-php": "dev-dev",
        "dorcas/sdk-php-laravel": "dev-dev",
        "fedeisas/laravel-mail-css-inliner": "^2.2",
        "fideloper/proxy": "~4.0",
        "guzzlehttp/guzzle": "^6.3",
        "kingflamez/laravelrave": "^1.0",
        "laravel/framework": "^6.0",
        "laravel/helpers": "^1.4",
        "laravel/passport": "^7.5",
        "laravel/tinker": "~1.0",
        "lcobucci/jwt": "^3.4",
        "league/csv": "^9.1",
        "moneyphp/money": "^3.1",
        "moontoast/math": "1.1.2 as 1.99.0",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^3.7",
        "yabacon/paystack-php": "^2.1"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Dorcas/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php artisan vendor:publish --tag=dorcas-modules --force"
        ]
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "cache-files-ttl":0
    }
}
